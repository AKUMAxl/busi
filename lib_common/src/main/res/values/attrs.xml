<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgressBar" >
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"></attr>
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="ClearnEditText">
        <attr name="headPic" format="reference"></attr>
        <attr name="type">
            <enum name="user" value="0"></enum>
            <enum name="password" value="1"></enum>
        </attr>

    </declare-styleable>

    <declare-styleable name="XlTitle">
        <attr name="titletext" format="string"></attr>
        <attr name="leftbg" format="reference"></attr>
        <attr name="rightbg" format="reference"></attr>
        <attr name="leftvisiable" format="boolean"></attr>
        <attr name="rightvisiable" format="boolean"></attr>
    </declare-styleable>



</resources>